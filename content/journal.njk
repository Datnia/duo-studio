---
layout: layouts/base.njk
eleventyComputed:
  title: Journal | {{ metadata.title }}
description: A collection of our thoughts, news and awards.
---

<div class="barba-container loading" data-barba="container" data-barba-namespace="journal" id="smooth-wrapper">
    <main id="smooth-content">
        <section id="banner">
            <div class="section__wrapper">
                <h1 class="top headline__load">Duo's Journal</h1>
            </div>
        </section>
        <section id="index">
            <div class="section__wrapper">
                <div class="row">
                    <p>
                        {% set postslistCounter = collections.posts | length %}
                        {{ postslistCounter }} Journal Entries
                    </p>
                    <ul>
                        <li>All</li>
                        <li>Insights</li>
                        <li>News</li>
                        <li>Resources</li>
                    </ul>
                </div>
            </div>
            <div class="posts">
                <div class="st__line"></div>
                {% set postslist = collections.posts %}
                {% include "postslist.njk" %}
            </div>
        </section>
        <div class="footer-spacer footer-trigger">
            <footer>
                <div class="grid">
                    <h4 class="st__headline headline__footer lg">Start a project</h4>
                    <a class="btn__circle" href="/contact">
                        <p>Get in Touch</p>
                    </a>
                    <h2>Create an experience thatâ€™s as remarkable as your business.</h2>
                </div>
                <div class="row">
                    <div class="col">
                        <span>Main Hubs</span>
                        <p>DC, Maryland, Virginia</p>
                    </div>
                    <div class="col">
                        <span>General Inquiries</span>
                        <a href="mailto:hello@duo-studio.co" class="cursor__hover --highlight">hello@duo-studio.co</a>
                    </div>
                    <div class="col">
                        <span>Follow Us</span>
                        <div class="row">
                            <a
                                href="https://www.instagram.com/duostudio_co/"
                                target="_blank"
                                rel="noopener noreferrer"
                                class="cursor__hover --highlight">Instagram,</a>
                            <a
                                href="https://www.linkedin.com/company/duostudio/"
                                target="_blank"
                                rel="noopener noreferrer"
                                class="cursor__hover --highlight">LinkedIn</a>
                        </div>
                    </div>
                    <div class="col copyright subtitle">
                        &copy;2023 Duo Studio
                        <a class="cursor__hover --highlight" href="/privacy-policy">Privacy Policy</a>
                    </div>
                </div>
            </footer>
        </div>
    </main>
</div>